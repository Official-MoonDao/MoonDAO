{
  "auth": {
    "username": "${BROWSERSTACK_USERNAME}",
    "access_key": "${BROWSERSTACK_ACCESS_KEY}"
  },
  "browsers": [
    {
      "os": "Windows 10",
      "browser": "chrome",
      "versions": ["latest", "latest-1"]
    },
    {
      "os": "Windows 10",
      "browser": "edge",
      "versions": ["latest"]
    },
    {
      "os": "OS X Big Sur",
      "browser": "webkit",
      "versions": ["latest"]
    },
    {
      "os": "Windows 10",
      "browser": "firefox",
      "versions": ["latest-1"]
    }
  ],
  "run_settings": {
    "parallels": 5,
    "npm_dependencies": {
      "cypress": "latest",
      "browserstack-cypress-cli": "latest",
      "typescript": "latest",
      "@types/node": "latest",
      "@types/react": "latest",
      "@types/react-dom": "latest"
    },
    "cypress_config_file": "cypress.browserstack.config.js",
    "specs": "cypress/e2e/**/*.cy.{js,ts,jsx,tsx}",
    "baseUrl": "http://localhost:3000",
    "spec_timeout": 1800
  },
  "connection_settings": {
    "local": true,
    "local_identifier": "moon-dao-tests"
  }
}
